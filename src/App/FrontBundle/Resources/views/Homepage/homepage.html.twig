            <div class="col-md-12 text-center">
                <h1><a name="testimonios">{% trans %}TESTIMONIOS{% endtrans %}</a></h1>
            </div>
        </div><!-- end row -->
    </section><!-- end section -->

    <div class="row">
        <br><br>

        <div class="col-md-8 col-md-offset-2 text-center">
        </div>
    </div>

    <div class="container">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="section-title text-center">
                        <h4>{% trans %}Actualmente somos más de 1000 usuarios y seguimos creciendo!.{% endtrans %}</h4>

                        <h1>{% trans %}Estas son algunas de las opiniones de nuestros clientes...{% endtrans %}</h1>
                        <hr>
                    </div><!-- end title -->
                </div>
            </div>
        </div><!-- end container -->
    </div>

    <section class="section lb">
        <div class="container-fluid">
            <div class="portfolio-wrapper">
                <div class="row">
                    {% for video in testimonios %}

                        <div class="col-sm-4 wow fadeIn" data-wow-duration="1s" data-wow-delay="0.2s">
                            <div class="media-element">
                                <video class="videoplayer" controls preload="none">
                                    <!-- MP4 for Safari, IE9, iPhone, iPad, Android, and Windows Phone 7 -->
                                    <source type="video/mp4" src="{{ app.request.basePath ~ '/' ~ video.getWebPath() }}" />
                                </video>
                            </div><!-- end media -->
                        </div>

                    {% endfor %}
                </div>
            </div><!-- end row -->
        </div><!-- end container -->
    </section><!-- end section -->


    <div class="row">
        <br><br>
        <div class="col-md-8 col-md-offset-2 text-center">
            <a href="#" class="btn btn-primary btn-block btn-lg">{% trans %}Ver más testimonios{% endtrans %}</a>
            <!-- <a href="#" class="btn btn-default">Empieza Ahora!</a>-->
        </div>
    </div>

    <div class="row">
        <br><br>

        <div class="col-md-8 col-md-offset-2 text-center">
        </div>
    </div>



{% endblock %}